{
  "expo": {
    "name": "Slumber",
    "slug": "Slumber",
    "version": "1.0.0",
    "orientation": "portrait",
  "icon": "./assets/images/app_icon.png",
    "scheme": "slumber",
    "userInterfaceStyle": "automatic",
  "jsEngine": "jsc",
  "newArchEnabled": false,
    "ios": {
      "icon": "./assets/images/app_icon.png",
      "supportsTablet": true,
      "newArchEnabled": false,
      "buildNumber": "7",
      "bundleIdentifier": "com.dangelo137.Slumber",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Camera only if you choose to use it.",
        "NSMicrophoneUsageDescription": "Mic only if you record audio.",
        "NSPhotoLibraryUsageDescription": "Photos only if you pick from library.",
        "NSPhotoLibraryAddUsageDescription": "Photos only if you save to library."
      }
    },
    "android": {
      "icon": "./assets/images/app_icon.png",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "SCHEDULE_EXACT_ALARM"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugiins": [
      "expo-router",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5de36dfc-c850-44c2-9b5d-a6ace39f502d"
      }
    }
  }
}
